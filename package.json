    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.2.0",
    "body-parser": "^1.14.1",
    "cheerio": "^0.19.0",
    "classnames": "^2.2.0",
    "connect-mongodb-session": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "ect": "^0.5.9",
    "express": "^4.13.3",
    "express-jsonschema": "^1.1.0",
    "express-session": "^1.12.1",
    "gitbook-plugin-anchors": "^0.4.0",
